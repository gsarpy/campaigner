{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node ./server/index.js",
    "server": "nodemon ./server/index.js",
    "client": "npm run start --prefix ./server/client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix ./server/client && npm run build --prefix ./server/client"
  },
  "author": "Gregory Sarpy",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.1",
    "concurrently": "^3.5.0",
    "cookie-session": "^1.3.1",
    "express": "^4.15.4",
    "mongoose": "^4.11.11",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "stripe": "^5.0.0"
  },
  "description": "Email campaign system",
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-config-fbjs": "^2.0.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-relay": "0.0.19"
  }
}
